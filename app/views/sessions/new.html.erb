<div>
  <h1>Log in</h1>
</div>

<div>
  <div>

    <%= form_with(url: login_path, scope: :session) do |f| %>

      <div>
        <%= f.label :email, 'Email' %>
        <%= f.email_field :email %>
      </div>

      <div>
        <%= f.label :password, 'Password' %>
        <%= f.password_field :password %>
      </div>

      <%= f.submit 'Log in' %>
    <% end %>

  </div>
</div>

<%= link_to '新規登録はこちら', signup_path %>